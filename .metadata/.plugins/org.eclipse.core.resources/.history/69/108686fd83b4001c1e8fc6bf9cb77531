#include <stdio.h>
#include <stdlib.h>

void mostrarTexto(char *a);

int main(void) {

	setbuf(stdout, NULL);
	mostrarTexto("Hola");

	return EXIT_SUCCESS;
}

void mostrarTexto(char *a)
{
	printf(*a);
}
