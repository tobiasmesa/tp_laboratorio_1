#include <stdio.h>
#include <stdlib.h>
#include "utn.h"

int main(void) {
	setbuf(stdout, NULL);
	float a;
	utn_getNumeroFlotante(&a, "\nIngrese el numero flotante", "\nMal. reingrese", 0, 100, 3);
	printf("El numero es: %0.5f", a);
	return EXIT_SUCCESS;
}
